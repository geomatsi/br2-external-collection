# wireless core
CONFIG_WIRELESS=y
CONFIG_CFG80211=y
CONFIG_MAC80211=y
CONFIG_CFG80211_WEXT=y

# wireless drivers
CONFIG_WLAN=y
CONFIG_WLAN_VENDOR_TI=y
CONFIG_WL12XX=m
CONFIG_WL18XX=m
CONFIG_WLCORE=m
CONFIG_WLCORE_SDIO=m
CONFIG_BT_HCIUART_LL=m

# remoteproc
CONFIG_REMOTEPROC=y
CONFIG_IMX_REMOTEPROC=m
CONFIG_REMOTEPROC_CDEV=y

# rpmsg
CONFIG_RPMSG=m
CONFIG_RPMSG_CHAR=m
CONFIG_RPMSG_VIRTIO=m

# imx firmware
CONFIG_IMX_SDMA=m
